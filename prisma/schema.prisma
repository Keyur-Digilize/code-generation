model PrinterLineSetting {
  id                       String                   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  printer_line_id          String                   @db.Uuid
  label                    String                   @db.VarChar(50)
  delay                    Int?
  date_format              String
  no_of_groups             Int                      @default(1)
